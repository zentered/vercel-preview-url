{
  "name": "vercel-preview-url",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to receive the deployment preview from Vercel",
  "keywords": [
    "vercel",
    "deployment",
    "github",
    "action"
  ],
  "homepage": "https://github.com/zentered/vercel-preview-url#readme",
  "bugs": {
    "url": "https://github.com/zentered/vercel-preview-url/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zentered/vercel-preview-url.git"
  },
  "license": "MIT",
  "author": "Zentered <hi@zentered.co> (https://zentered.co)",
  "contributors": [
    "Patrick Heneise (https://github.com/zentered)"
  ],
  "main": "dist/index.js",
  "scripts": {
    "all": "npm run lint && npm run test && npm run prepare",
    "lint": "eslint .",
    "prepare": "ncc build index.mjs -o dist --source-map --license licenses.txt",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,json,md}": [
      "prettier --write"
    ],
    "*.{mjs,js}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "axios": "^0.21.1",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vercel/ncc": "^0.27.0",
    "babel-jest": "^26.6.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
